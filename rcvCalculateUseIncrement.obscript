scn rcvCalculateUseIncrement

; inputs
float useInc
float useIncModifier

; ouput
float modifieduseInc

Begin Function { useInc useIncModifier } ; Gets all skill use overrides for a particular Recurved.SkillUseOverrideReasons overrideReason.

	if (useIncModifier > 0)
		set modifieduseInc to useInc / useIncModifier
	else
		set modifieduseInc to 0.0
	endif
	SetFunctionValue modifieduseInc
	
End