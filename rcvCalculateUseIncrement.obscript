scn rcvCalculateUseIncrement

; inputs
float useInc
float useIncModifier

; ouput
float modifieduseInc

Begin Function { useInc useIncModifier } ; Calculates a use increment by dividing by the useIncModifier if greater than zero, otherwise returns zero.

	if (useIncModifier > 0)
		set modifieduseInc to useInc / useIncModifier
	else
		set modifieduseInc to 0.0
	endif
	SetFunctionValue modifieduseInc
	
End