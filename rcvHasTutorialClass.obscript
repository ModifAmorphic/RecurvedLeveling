scn rcvHasTutorialClass

ref class

; Output
short isTutorialClass
Begin _Function {  } ; Checks if the player character has the CharactergenClass class assigned.
	let class := player.GetClass
	if class != CharactergenClass
		set isTutorialClass to 0
	else
		set isTutorialClass to 1
	endif
	SetFunctionValue isTutorialClass
End