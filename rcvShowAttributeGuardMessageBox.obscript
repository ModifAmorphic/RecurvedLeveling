scn rcvShowAttributeGuardMessageBox

string_var replacer
string_var formattedMessage
;input params
string_var dispMessage
int minimumMaxedAttributes
Begin _Function { dispMessage minimumMaxedAttributes }
	let formattedMessage := sv_Construct "%z" dispMessage
	let replacer := sv_Construct "{{MinimumMaxedAttributes}}|%g" minimumMaxedAttributes
	sv_Replace $replacer formattedMessage
	MessageBoxEx formattedMessage

	sv_Destruct replacer
	sv_Destruct formattedMessage
	sv_Destruct dispMessage
End