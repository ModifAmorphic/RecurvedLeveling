scn rcvGetHighestSkillUse


float useInc1

;input
int skillCode

;output
float skilluseInc
Begin _Function { skillCode }
	set skilluseInc to GetSkillUseIncrementC skillCode 0
	set useInc1 to GetSkillUseIncrementC skillCode 1
	if (useInc1 > skilluseInc)
		set skilluseInc to useInc1
	endif
	SetFunctionValue skilluseInc
End